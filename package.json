{
  "name": "vercel-web-scripts",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidKk/vercel-web-scripts"
  },
  "scripts": {
    "commit": "git-cz",
    "ci:coverage": "pnpm test",
    "postinstall": "node --experimental-modules ./scripts/postinstall.mjs && node scripts/generate-editor-typings.mjs",
    "preinstall": "node --experimental-modules ./scripts/preinstall.mjs",
    "clear": "rimraf --glob ./libs",
    "format": "prettier --config .prettierrc.js --write \"**/*.{js,jsx,ts,tsx,d.ts,vue,md,json,yml,yaml}\"",
    "lint": "pnpm lint:ts",
    "lint:ts": "eslint -c eslint.config.mjs --fix .",
    "typecheck": "tsc --noEmit && pnpm run typecheck:preset",
    "typecheck:preset": "cd preset && tsc --noEmit",
    "dev": "pnpm run generate:editor-typings && pnpm run build:preset && concurrently -n next,preset \"next dev --webpack\" \"pnpm run build:preset:dev\"",
    "build": "pnpm run build:preset && next build --webpack",
    "start": "next start",
    "test": "jest -c ./jest.config.ts --coverage --reporters=default --reporters=jest-junit --passWithNoTests",
    "test:watch": "jest --watch",
    "ok": "pnpm format && pnpm lint && pnpm typecheck && pnpm test",
    "test:e2e": "playwright test --reporter=list",
    "dev:e2e": "next dev --hostname 127.0.0.1 --port 3000",
    "generate:editor-typings": "node scripts/generate-editor-typings.mjs",
    "build:preset": "vite build --config preset/vite.config.ts && pnpm run generate:editor-typings",
    "build:preset:dev": "vite build --config preset/vite.config.ts --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.10.0",
    "@codemirror/state": "^6.4.1",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.23.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ahooks": "^3.8.4",
    "cookie": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "16.1.4",
    "otplib": "^12.0.1",
    "prettier": "^3.4.2",
    "puppeteer": "^24.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "ts-morph": "^26.0.0",
    "@iconify-json/mdi": "^1.2.3",
    "typescript": "^5.7.2",
    "unplugin-icons": "^23.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3",
    "@jest/types": "^29.6.3",
    "@playwright/test": "^1.56.1",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "chalk": "^5.3.0",
    "commitizen": "^4.2.4",
    "commitlint-plugin-i18n": "^0.0.1-alpha.3",
    "concurrently": "^9.1.2",
    "cz-customizable": "^7.2.1",
    "cz-customizable-i18n-config": "^0.0.1-alpha.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-runner-tsd": "^6.0.0",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "pretty-error": "^4.0.0",
    "rimraf": "^6.0.1",
    "spinnies": "^0.5.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsd-lite": "^0.9.0",
    "typescript": "^5.7.2",
    "typescript-transform-paths": "^3.4.7",
    "vite": "^6.0.0"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316"
}
